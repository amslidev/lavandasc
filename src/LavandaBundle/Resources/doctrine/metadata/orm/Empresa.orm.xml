<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Empresa" table="empresa">
    <id name="idempresa" type="integer" column="idEmpresa">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="razonsocial" type="string" column="razonSocial" length="150" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="direccionfiscal" type="text" column="direccionFiscal" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="representante" type="string" column="representante" length="150" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="telefonorepresentante" type="string" column="telefonoRepresentante" length="10" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="rfc" type="string" column="rfc" length="13" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="rutalogo" type="string" column="rutaLogo" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nombrelogo" type="string" column="nombreLogo" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="mision" type="text" column="mision" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="vision" type="text" column="vision" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="valores" type="text" column="valores" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
